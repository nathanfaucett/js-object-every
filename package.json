{
  "name": "@nathanfaucett/object-every",
  "version": "0.0.2",
  "description": "tests whether all elements in the object pass the test implemented by the provided function.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/object-every.git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "object-every",
    "object-every.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-jsbeautifier": "^0.2.13",
    "tap-spec": "^4.0.0",
    "tape": "^4.0.0",
    "zuul": "^3.0.0"
  },
  "dependencies": {
    "@nathanfaucett/keys": "^0.0.2"
  }
}
